(defvar *captures*)
(defvar *program*)

(defparameter *instructions* '())

(defmacro instr (name expr)
  (let ((real-name (intern (format nil "INSTR-~a" name))))
    `(progn
       (defun ,real-name (regs a b c)
         (declare (ignorable b))
         (let ((result (copy-list regs)))
           (flet ((r (i) (elt regs i)))
             (declare (ignorable #'r))
             (setf (elt result c) ,expr))
           result))
       (push #',real-name *instructions*))))

(instr addr (+ (r a) (r b)))
(instr addi (+ (r a) b))
(instr mulr (* (r a) (r b)))
(instr muli (* (r a) b))
(instr banr (logand (r a) (r b)))
(instr bani (logand (r a) b))
(instr borr (logior (r a) (r b)))
(instr bori (logior (r a) b))
(instr setr (r a))
(instr seti a)
(instr gtir (if (> a (r b)) 1 0))
(instr gtri (if (> (r a) b) 1 0))
(instr gtrr (if (> (r a) (r b)) 1 0))
(instr eqir (if (= a (r b)) 1 0))
(instr eqri (if (= (r a) b) 1 0))
(instr eqrr (if (= (r a) (r b)) 1 0))

(defun compatible-instructions (capture)
  (destructuring-bind (i a b c)
      (second capture)
    (declare (ignore i))
    (remove-if-not (lambda (instr)
                     (equal (funcall instr (first capture) a b c)
                            (third capture)))
                   *instructions*)))

(defun adv16 ()
  (length (remove-if-not (lambda (c)
                           (>= (length (compatible-instructions c)) 3))
                         *captures*)))

(defun find-instruction (opcode)
  (let* ((captures (remove-if-not (lambda (c)
                                    (= (car (second c)) opcode))
                                  *captures*))
         (compatibles (mapcar #'compatible-instructions captures)))
    (reduce #'intersection compatibles)))

(defun instruction-set ()
  (let ((result '()))
    (labels ((rec (insts)
               (let ((next (find 1 insts :key (lambda (x) (length (cdr x))))))
                 (when next
                   (push (cons (car next) (cadr next)) result)
                   (rec (mapcar (lambda (x)
                                  (remove (cadr next) x))
                                insts))))))
      (rec (loop for i from 0 below 16 collect (cons i (find-instruction i)))))
    result))

(defun adv16b ()
  (let ((instructions (instruction-set)))
    (labels ((rec (program regs)
               (if (null program)
                   (car regs)
                   (rec (cdr program)
                        (apply (cdr (assoc (caar program) instructions))
                               regs (cdar program))))))
      (rec *program* '(0 0 0 0)))))

